{
    "configuration-fill": "class",
    "configuration-skip": "NOTE",
    "interpolation": "linear",



    "class": {
        "NOTE": "Parameter classes (should) mirror https://software.ecmwf.int/wiki/display/MIR/Parameters+classification",
        "FIXME": {
            "direction-associated-to-wave-spectra": {
                "class": "direction",
                "interpolation": "nearest-neighbour"
            },
            "intensity-associated-to-wave-spectra": {
