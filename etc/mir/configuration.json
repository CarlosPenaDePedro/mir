            },
            "wave-spectra": {
                "class": "unknown",
                "interpolation": "nearest-neighbour"
            }
        },
        "concentration": {
            "NOTE": "composition of a mixture (mass, amount, volume, ...) with respect to the volume of the mixture (typical units: [... m**-3])"
        },
        "direction": {
            "FieldInfo": "vector.1d.polar.angle",
            "NOTE": "planar angle (typical units: radians or degrees)",
            "decomposition": "PolarAngleDegreesAssymmetricToCartesian",
            "degree": "true",
            "stats": "AngleDecomposition",
            "symmetric": "false"
        },
        "flux": {
            "NOTE": "rate of flow of a property per unit area (typical units: [... s**-1 m**-2])"
        },
        "index": {
            "NOTE": "associated to a table",
            "interpolation": "nearest-neighbour"
        },
        "intensity": {
            "NOTE": "physical quantity whose magnitude is independent of the extent (size) of the system (typical case, no typical units)",
            "comparison": "scalar/scalar"
        },
        "rate": {
            "NOTE": "ratio between two related quantities (time, mass, length, etc., typical units: [... s**-1])"
        },
        "ratio": {
            "NOTE": "quotient of quantities of the same kind (typical units: <non-dimensional>)",
            "cover": {
                "NOTE": "ratio representing a box average (typical units: <non-dimensional>)"
            },
            "fraction": {
                "NOTE": "(typical units: <non-dimensional>, [0, 1], [-1, 1]?)"
            },
            "percentage": {
                "NOTE": "(typical units: <non-dimensional>, %)"
            },
            "probability": {
                "NOTE": "(typical units: <non-dimensional>, [0, 1])"
            }
        },
        "unknown": {
            "FIXME": "issue warning?"
        }
    },


