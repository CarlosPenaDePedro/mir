#include "mir/api/mir_version.h"

const char * mir_version()     { return MIR_VERSION; }
const char * mir_version_str() { return MIR_VERSION_STR; }

unsigned int mir_version_int()
{
  return 10000*MIR_MAJOR_VERSION + 100*MIR_MINOR_VERSION + 1*MIR_PATCH_VERSION;
}

const char * mir_git_sha1() { return "@MIR_GIT_SHA1@"; }

